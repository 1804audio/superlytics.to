    "is-ci": "^4.1.0",
    "is-docker": "^3.0.0",
    "is-localhost-ip": "^2.0.0",
    "isbot": "^5.1.28",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "maxmind": "^4.3.28",
    "md5": "^2.3.0",
    "next": "15.4.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prisma": "6.12.0",
